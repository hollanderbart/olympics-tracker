{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["esnext"],
    "types": ["@cloudflare/workers-types"],
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "paths": {
      "@/*": ["../*"]
    }
  },
  "include": ["./**/*.ts"],
  "exclude": ["node_modules"]
}
